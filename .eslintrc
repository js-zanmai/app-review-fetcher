{
  "rules": {
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "no-console": [0],
    "no-multi-spaces": [2],
    "no-var": [2],
    "no-unused-vars": [1]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}